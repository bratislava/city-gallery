{
  "root": "apps/next/city-gallery",
  "sourceRoot": "apps/next/city-gallery",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/next/city-gallery",
        "outputPath": "dist/apps/next/city-gallery"
      },
      "configurations": {
        "production": {
          "generatePackageJson": true
        }
      },
      "dependsOn": [
        {
          "target": "typecheck",
          "projects": "self"
        }
      ]
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "next-city-gallery:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "next-city-gallery:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "next-city-gallery:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/next/city-gallery"],
      "options": {
        "jestConfig": "apps/next/city-gallery/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/next/city-gallery/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "docker-build": {
      "executor": "./tools/executors/docker:build",
      "options": {
        "technology": "next"
      }
    },
    "docker-run": {
      "executor": "./tools/executors/docker:run",
      "options": {
        "ports": ["3000:3000"]
      }
    },
    "add-secret": {
      "executor": "./tools/executors/kustomize:secrets",
      "options": {}
    },
    "dev": {
      "executor": "./tools/executors/strapi:dev",
      "options": {
        "strapiProject": "strapi-city-gallery"
      }
    },
    "typecheck": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "cd ./apps/next/city-gallery && tsc --noEmit --pretty && tsc-strict"
      },
      "configurations": {
        "production": {}
      }
    },
    "update-strict-comments": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "cd ./apps/next/city-gallery && update-strict-comments"
      }
    }
  },
  "tags": []
}
