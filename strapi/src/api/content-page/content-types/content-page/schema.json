{
  "kind": "collectionType",
  "collectionName": "content_pages",
  "info": {
    "singularName": "content-page",
    "pluralName": "content-pages",
    "displayName": "Content Pages",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "subtitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "perex": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "coverMedia": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "color": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "inheritColorFromParent": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false
    },
    "mainContent": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": ["sections.richtext-section", "sections.video-section"]
    },
    "slider": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.slider-section"
    },
    "relatedContentTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "relatedContentSubmenuTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "downloadSection": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.download-section"
    },
    "dateFrom": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "date"
    },
    "dateTo": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "date"
    },
    "timeFrom": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "time"
    },
    "timeTo": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "time"
    },
    "showRemainingTime": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": true
    },
    "placeTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "placeAddress": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "purchaseId": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "positions": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "blocks.position-item"
    },
    "partners": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "blocks.partner-item"
    },
    "override": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "blocks.highlight-override"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "blocks.seo"
    },
    "place": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::place.place",
      "inversedBy": "contentPages"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "contentPages"
    },
    "parentPage": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::content-page.content-page",
      "inversedBy": "childPages"
    },
    "childPages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::content-page.content-page",
      "mappedBy": "parentPage"
    }
  }
}
